<!--door 7x4 (powered)-->
<block name="Sonic's Roll Up Door 7x4(powered)">
	<!-- Class -->
	<property name="Class" value="PoweredDoor"/>
	<property name="CustomIcon" value="rollUpDoor7x4White"/>
